{
  "_version": "1.12.0",
  "sap.app": {
    "id": "crickettournament.teamsui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "tablet": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "rootView": {
      "viewName": "crickettournament.teamsui.view.Teams",
      "type": "XML",
      "async": true,
      "id": "teamsRootView"
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "autoExpandSelect": true,
          "synchronizationMode": "None",
          "operationMode": "Server",
          "serviceUrl": "/odata/v4/cricket/",
          "withCredentials": true
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "teamsPage",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "teams"
        }
      ],
      "targets": {
        "teams": {
          "viewId": "teamsView",
          "viewName": "teamsui.view.Teams",
          "viewType": "XML"
        }
      }
    }
  }
}
